<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_common" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <script src="/odoo_fart/static/lib/fartscroll.js" defer="defer" async="async"></script>
            <script src="/odoo_fart/static/src/js/main.js" defer="defer" async="async"></script>
        </xpath>
    </template>


    <template id="assets_backend"  inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script src="/odoo_fart/static/lib/fartscroll.js" defer="defer" async="async"></script>
            <script src="/odoo_fart/static/src/js/main.js" defer="defer" async="async"></script>
        </xpath>
    </template>
</odoo>
